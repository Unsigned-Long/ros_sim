<launch>
    <param name="robot_description" command="$(find xacro)/xacro $(find scene)/urdf/sim-robot.xacro" />

    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find scene)/config/rviz-config.rviz" />

    <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher" />
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />

    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find scene)/world/sim-scene.world" />
    </include>

    <node pkg="gazebo_ros" type="spawn_model" name="model" args="-urdf -model mycar -param robot_description" />

    <!-- <node pkg="scene" name="scene_node" type="scene_node" output="screen" /> -->
</launch>